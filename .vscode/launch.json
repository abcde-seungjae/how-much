{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost에 대해 Chrome 시작",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "스크립트 실행: start",
      "request": "launch",
      "command": "yarn start",
      "cwd": "${workspaceFolder}"
    }
  ]
}